@if(Model != null)
{
    if(Model.Count == 0)
    {
        <tr>
            <td colspan="2">No Data to show</td>
        </tr>
    }
    else
    {
        @foreach (var student in Model)
        {
            <tr>
                <td>
                    @student.firstname @student.middlename @student.lastname
                </td>
            </tr>
        }
    }
   
}
